{
  "name": "@dukarun/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/src/index.js",
    "start:worker": "node ./dist/src/index-worker.js",
    "start": "concurrently npm:start:*",
    "migration:run": "node -e \"require('@vendure/core').runMigrations(require('./dist/src/vendure-config').config).then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "entrypoint": "ts-node ./src/entrypoint.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:migrations": "jest --testNamePattern='Migration Idempotence'",
    "test:all": "npm run build && npm run test:coverage",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.209.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.209.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.209.0",
    "@opentelemetry/instrumentation": "^0.209.0",
    "@opentelemetry/instrumentation-express": "^0.57.1",
    "@opentelemetry/instrumentation-graphql": "^0.56.0",
    "@opentelemetry/instrumentation-http": "^0.209.0",
    "@opentelemetry/instrumentation-ioredis": "^0.57.0",
    "@opentelemetry/instrumentation-pg": "^0.61.2",
    "@opentelemetry/resources": "^2.3.0",
    "@opentelemetry/sdk-logs": "^0.209.0",
    "@opentelemetry/sdk-metrics": "^2.3.0",
    "@opentelemetry/sdk-node": "^0.209.0",
    "@opentelemetry/sdk-trace-base": "^2.3.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@vendure/admin-ui-plugin": "3.5.2",
    "@vendure/asset-server-plugin": "3.5.2",
    "@vendure/common": "3.5.2",
    "@vendure/core": "3.5.2",
    "@vendure/create": "3.5.2",
    "@vendure/email-plugin": "3.5.2",
    "@vendure/graphiql-plugin": "3.5.2",
    "axios": "^1.13.2",
    "concurrently": "9.2.1",
    "dotenv": "17.2.3",
    "ioredis": "^5.9.1",
    "p-limit": "^7.2.0",
    "pg": "8.16.3",
    "request-ip": "^3.3.0",
    "type-graphql": "^2.0.0-rc.2",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.16.0",
    "@types/request-ip": "^0.0.41",
    "@types/web-push": "^3.6.4",
    "@vendure/cli": "3.5.2",
    "@vendure/ui-devkit": "^3.5.2",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3"
  }
}
