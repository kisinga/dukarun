{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/logo/**", "/*.(eot|svg|cur|jpg|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "screenshots-freshness",
      "urls": ["/assets/screenshots/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 500,
        "maxAge": "1h",
        "timeout": "10s"
      }
    },
    {
      "name": "api-freshness",
      "urls": ["/admin-api/**", "/shop-api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "10s"
      }
    },
    {
      "name": "api-performance",
      "urls": ["/admin-api/assets/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d"
      }
    }
  ],
  "navigationUrls": ["/", "/**", "!**/admin-api/**", "!**/shop-api/**", "!**/*.*", "!**/*__*"],
  "navigationRequestStrategy": "freshness"
}
