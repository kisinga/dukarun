{
  "name": "@dukarun/frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "USE_HEADLESS=true ng test --watch=false",
    "test:ci": "CI=true ng test --code-coverage --watch=false",
    "test:coverage": "ng test --code-coverage --watch=false",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts",
    "lint": "ng lint",
    "format": "prettier --write \"**/*.{ts,js,json,html,scss,css}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,html,scss,css}\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/pwa": "^21.0.5",
    "@angular/router": "^21.0.8",
    "@angular/service-worker": "^21.0.8",
    "@apollo/client": "^4.0.12",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.209.0",
    "@opentelemetry/instrumentation": "^0.209.0",
    "@opentelemetry/instrumentation-fetch": "^0.209.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.209.0",
    "@opentelemetry/resources": "^2.3.0",
    "@opentelemetry/sdk-trace-base": "^2.3.0",
    "@opentelemetry/sdk-trace-web": "^2.3.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tensorflow/tfjs": "^4.22.0",
    "apollo-angular": "^13.0.0",
    "daisyui": "^5.5.14",
    "graphql": "^16.12.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@types/jasmine": "~5.1.14",
    "@types/node": "^25.0.6",
    "buffer": "^6.0.3",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  }
}
